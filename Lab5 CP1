//A program that takes two sides  of a triangle and gives the third sides value as output
#include <stdio.h>
#include <math.h>

int main (void)
{
    //Defining Variables
    float a, b, c;

    //Taking Inputs
    printf("Enter the value of a and b: ");
    scanf("%f%f", &a, &b);

    //Computation
    c = sqrt(pow(b,2)+ pow(b,2));

    //Result
    printf("The value of c is %f ", c);


    return 0;
}
